<main>
    <article>
            <ul>
                <%@users.each do |user|%>
                    <li><a href="/users/<%=user.id%>"><%=user.id%>. <%= user.username %></a></li>
                <%end%>
            </ul>
            <h2>Number of Users: <%=@user_count%></h2>
    </article>
</main>
<section>
    <ul>
        <li><a href="/users/collection/<%=@first%>">First</a></li>
        <li><a href="/users/collection/<%=@previous%>">Previous</a></li>
        <li><a href="/users/collection/<%=@current_page_number%>"><%=@current_page_number%></a></li>
        <li><a href="/users/collection/<%=@next%>">Next</a></li>
        <li><a href="/users/collection/<%=@last%>">Last</a></li>
    </ul>
</section>